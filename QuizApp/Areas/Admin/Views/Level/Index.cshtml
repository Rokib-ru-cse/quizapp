@model IEnumerable<Level>
<div class="container">
    <a class="btn btn-outline-primary my-2" asp-action="Create">Create Level</a>
    <div class="row">
        <div class="col-md-8 offset-2">
            <h1 class="text-center">Level list</h1>
            <table class="table table-striped table-dark">
                <thead>
                    <tr>
                        <th>Name</th>
                        <th>Subjects</th>
                        <th>Details</th>
                        <th>Edit</th>
                        <th>Delete</th>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var item in Model)
                    {
                        <tr>
                            <td>@item.Name</td>
                            <td><a>Subjects</a></td>
                            <td><a asp-action="Details" asp-route-id="@item.Id">Details</a></td>
                            <td><a asp-action="Edit" asp-route-id="@item.Id">Edit</a></td>
                            <td><a asp-action="Delete" asp-route-id="@item.Id">Delete</a></td>
                        </tr>
                    }
                </tbody>
            </table>
        </div>
    </div>
</div>
