@model IEnumerable<Subject>

<div class="container">
    <div class="row">
        <div class="col-md-8 offset-2">
            <h1 class="text-center">Subject list</h1>
            <table class="table table-striped table-dark">
                <thead>
                    <tr>
                        <th>Name</th>
                        <th>Levels</th>
                        <th>Edit</th>
                        <th>Delete</th>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var item in Model)
                    {
                        <tr>
                            <td>@item.Name</td>
                            <td><a>Levels</a></td>
                            <td>Edit</td>
                            <td>Delete</td>
                        </tr>
                    }
                </tbody>
            </table>
        </div>
    </div>
</div>